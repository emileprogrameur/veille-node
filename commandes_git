git remote add origin https://github.com/emileprogrameur/veille-node.git
git push -u origin master

git add "nom du fichier" //indexer un seul fichier
git add . //indexer l'ensemble des fichiers

git status //donne le status de l'arboressance
git status -s //donne le status de l'arboressance mais en résumé

git commit //updater les fichiers
-m //ajouter un commentaire au commit